function freePassClaim()
for i, v in pairs(getgc(true)) do
    if type(v) == "table" and rawget(v, "Remote") then
        if type(v["Remote"]) == "table" or type(v["Remote"]) == "userdata" then
            v["Remote"].Name = v["Name"]
        end
    end
end

for i = 1,20 do
local args = {
    [1] = i,
    [2] = "Main"
}

game:GetService("ReplicatedStorage"):WaitForChild("Communication"):WaitForChild("Functions"):WaitForChild("ClaimSeasonReward"):InvokeServer(unpack(args))
task.wait(0.5)
end
end

function PremiumPassClaim()
for i, v in pairs(getgc(true)) do
    if type(v) == "table" and rawget(v, "Remote") then
        if type(v["Remote"]) == "table" or type(v["Remote"]) == "userdata" then
            v["Remote"].Name = v["Name"]
        end
    end
end

for i = 1,20 do
local args = {
    [1] = i,
    [2] = "Premium"
}

game:GetService("ReplicatedStorage"):WaitForChild("Communication"):WaitForChild("Functions"):WaitForChild("ClaimSeasonReward"):InvokeServer(unpack(args))
task.wait(0.5)
end
end


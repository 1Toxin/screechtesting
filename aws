function autoIndex()
    local petsFromEggs = {
        ['Earth'] = {"Mouse", "Dog", "Cat", "Cow"},
        ['Icy'] = {"Fox", "Bear", "Deer", "Squirrel", "Slime"},
        ['Lava'] = {"Monkey", "Parrot", "Elephant", "Tiger"},
        ['Blackhole'] = {"GrassMage", "MagicMage", "WaterMage", "Spooky"},
        ['Crystal'] = {"Shark", "Penguin", "ArcticFox", "Wolf", "Orca"},
        ['Molten'] = {"Astra", "Unicorn", "Axeli", "Quasar"},
        ['Solar'] = {"SunRabbit", "SunBull", "SolarDog", "SunDeer"},
        ['Moon'] = {"MoonBat", "MoonOwl", "MoonSpider"},
        ['Ice'] = {"Rogue", "Scaler", "Drifter"},
        ['Burning'] = {"Tetra", "Dermite", "Magnus", "Vangaurd"},
        ['Coconut'] = {"Pinky", "Brutus", "Sour", "Poison", "Crimson"},
        ['Palm'] = {"CoCo", "Calypso", "Fang", "Barkley", "Raffia"},
        ['Treasure'] = {"Cutlass", "Kelp", "Coin", "Ruby"},
        ['Clam'] = {"Ascent", "Venti", "Tide", "Pearl"},
        ['KingFish'] = {"Hooky", "QueenTrident", "KingTrident"},
        ['Poseidon'] = {"Phoenix", "Thunder", "Bolt"},
        ['Rust'] = {"CowRaider", "Rex", "Welder", "Spike", "Rustie"},
        ['Widget'] = {"AT-10", "C-7", "BEEF-10", "E-1", "RAID-99"},
        ['Atom'] = {"Proto", "Neutrino", "Evangilo", "Alpha", "Alto"},
        ['Iridescent'] = {"Shirma", "Dimentro", "Veltro", "Expa"},
        ['Mutant'] = {"Ravage", "Sneak", "Squeek", "Grime"},
        ['Nuclear'] = {"Blaze", "LumoCat", "Spark", "LumoSheep", "LumoBoxer", "LumoDog"}
    }

    local notPet = {}

    local function updateNotPet()
        notPet = {}
        for _, lockedPet in pairs(game:GetService("Players").LocalPlayer.PlayerGui.GameUI.Menus.PetIndex.Container.ScrollingFrame.Pets:GetDescendants()) do
            if lockedPet.Name == "PetIcon" and lockedPet:IsA("ImageLabel") and lockedPet.ImageColor3 == Color3.new(0, 0, 0) then
                local petLocked = lockedPet.Parent.Parent.Name
                table.insert(notPet, petLocked)
            end
        end
    end

    while wait() and Config.autoIndex do
        updateNotPet()

        while #notPet > 0 and wait() and Config.autoIndex do
            for egg, _ in pairs(petsFromEggs) do
                local exclude = false
                if getgenv().Config.ExcludedWorld then
                    for _, zone in ipairs(worlds[getgenv().Config.ExcludedWorld]) do
                        if egg == zone then
                            exclude = true
                            break
                        end
                    end
                end
                
                if not exclude then
                    local args = {
                        [1] = egg,
                        [2] = {},
                        [4] = false
                    }
                    game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("EggService"):WaitForChild("RF"):WaitForChild("purchaseEgg"):InvokeServer(unpack(args))
                end                

                if not Config.autoIndex then
                    break
                end
            end
            updateNotPet()
            if not Config.autoIndex then
                break
            end
        end
    end
end

function setInv()
    if next(dontDeletePets) ~= nil then
        dontDeletePets = {}
    end

    for _, kpets in pairs(game:GetService("Players").LocalPlayer.PlayerGui.GameUI.Menus.PetInventory.Container.ScrollingFrame.Pets:GetChildren()) do
        if kpets:IsA("Frame") then
            dontDeletePets[kpets.Name] = true
        end
    end
end

function petDelete()
    if next(dontDeletePets) == nil then
        setInv()
    end

    while task.wait() and Config.petDelete and Config.autoIndex do
        for _, pet in pairs(game:GetService("Players").LocalPlayer.PlayerGui.GameUI.Menus.PetInventory.Container.ScrollingFrame.Pets:GetDescendants()) do
            if pet.Name == "Stat" and pet:IsA("TextLabel") and dontDeletePets[pet.Parent.Parent.Name] == nil then
                local args = {
                    [1] = pet.Parent.Parent.Name
                }

                game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PetService"):WaitForChild("RF"):WaitForChild("delete"):InvokeServer(unpack(args))
            end
        end
    end
end

local player = game.Players.LocalPlayer
local petInventory = player.PlayerGui.GameUI.Menus.PetInventory.Container.ScrollingFrame.Pets


local function deleteEventPet2(pet)
    local toggleButton = pet:FindFirstChild("Toggle")
    if toggleButton then
        local statLabel = toggleButton:FindFirstChild("Stat")
        if statLabel then
            local statText = statLabel.Text
            if statText == "Summer Cat" or statText == "Leviathan" or statText == "King Octopus" then
                local parentName = statLabel.Parent.Parent.Name
                return parentName
            end
        end
    end
end


function deletebadPets()
    local badPets = {"Holo Cat", "Holo Jellyfish", "Holo Croc"}
    
    for _, pets in pairs(game:GetService("Players").LocalPlayer.PlayerGui.GameUI.Menus.PetInventory.Container.ScrollingFrame.Pets:GetDescendants()) do
        if pets.Name == "Stat" and pets:IsA("TextLabel") then
            for _, badPet in pairs(badPets) do
                if pets.Text == badPet then
                    local args = {
                    [1] = pets.Parent.Parent.Name
                }
                
                game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PetService"):WaitForChild("RF"):WaitForChild("delete"):InvokeServer(unpack(args))
                end
            end
        end
    end
end

EventEgg:AddToggle({
	Name = "Delete Bad Pets",
	Default = false,
	Callback = function(Value)
	_G.EventDelete = Value
		while _G.EventDelete do
            wait()
			spawn(deletebadPets)
        end
	end    
})

-- Addon

Addon:AddParagraph("Instructions", "1. Locate Your Executors Workspace Folder\n2. Open The ScreechHub Folder and Open Addons (If You dont see it, re-execute script\n3. Create a .lua or .txt with the Addon name\n4. Put the Addon Code inside the file and re-execute\nTip. To Find Workspace Folder go to Appdata/local/packages/, then find a folder like this\n ROBLOXCORPORATION.ROBLOX_XXXXXXXXX/AC/Workspace)")

if makefolder and writefile and readfile and listfiles then
    makefolder("ScreechHub")
    makefolder("ScreechHub\\Addon")

    local addonFiles = listfiles("ScreechHub\\Addon")
    for _, file in ipairs(addonFiles) do
        local script = readfile(file)
        local fileName = file:gsub("ScreechHub\\Addon\\", "")
        fileName = fileName:gsub(".txt", ""):gsub(".lua", "")
        Addon:AddButton({
            Name = fileName,
            Callback = function()
                loadstring(script)()
            end    
        })
    end
end

local function worldtp(tpworld)
    if tpworld == "2" then
        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ZoneService"):WaitForChild("RE"):WaitForChild("teleport"):FireServer(workspace:WaitForChild("Zones"):WaitForChild("2"):WaitForChild("Interactables"):WaitForChild("Teleports"):WaitForChild("Locations"):WaitForChild("Futuristic"))
        elseif tpworld == "1" then
            game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ZoneService"):WaitForChild("RE"):WaitForChild("teleport"):FireServer(workspace:WaitForChild("Zones"):WaitForChild("1"):WaitForChild("Interactables"):WaitForChild("Teleports"):WaitForChild("Locations"):WaitForChild("BackToSchool"))
            elseif tpworld == "3" then
                game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ZoneService"):WaitForChild("RE"):WaitForChild("teleport"):FireServer(workspace:WaitForChild("Zones"):WaitForChild("3"):WaitForChild("Interactables"):WaitForChild("Teleports"):WaitForChild("Locations"):WaitForChild("Beach"))
                elseif tpworld == "4" then
                    game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("ZoneService"):WaitForChild("RE"):WaitForChild("teleport"):FireServer(workspace:WaitForChild("Zones"):WaitForChild("4"):WaitForChild("Interactables"):WaitForChild("Teleports"):WaitForChild("Locations"):WaitForChild("NuclearBunker"))
    end
end


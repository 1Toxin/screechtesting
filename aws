		_G.bagclick = Value
        while _G.bagclick do
            wait()
			if _G.SelectedBag == "VIP" then
			game:GetService("ReplicatedStorage").Packages._Index["sleitnick_knit@1.4.7"].knit.Services.PunchBagService.RE.onGiveStats:FireServer(_G.SelectedZone, _G.SelectedBag, true)
				elseif _G.SelectedBag ~= "VIP" then
				game:GetService("ReplicatedStorage").Packages._Index["sleitnick_knit@1.4.7"].knit.Services.PunchBagService.RE.onGiveStats:FireServer(_G.SelectedZone, _G.SelectedBag)
			end
        end
	end    
})

